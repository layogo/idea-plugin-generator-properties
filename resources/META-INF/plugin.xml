<idea-plugin>
  <id>cn.fisho.plugin.idea.generator.properties</id>
  <name>Generator Properties</name>
  <version>1.0</version>
  <!--<vendor email="fish.o@qq.com" url="http://www.fisho.cn">fisho</vendor>-->

  <description><![CDATA[
      Generator inner class properties by Java entity Class.<br>
      <em>.</em>
    ]]></description>

  <change-notes><![CDATA[
